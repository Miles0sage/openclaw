      "name": "MiniMax AI",
      "baseUrl": "https://api.minimax.chat/v1",
      "apiKeyEnv": "MINIMAX_API_KEY",
      "models": {
        "m2.5": {
          "name": "MiniMax-M2.5",
          "type": "text",
          "contextWindow": 1000000,
          "maxOutputTokens": 131072,
          "costPer1MInputTokens": 0.3,
          "costPer1MOutputTokens": 1.2,
          "benchmarks": {
            "sweBench": "80.2%",
            "mmlu": "87%",
            "toolCalling": "76.8%",
            "multiSweBench": "51.3%"
          }
